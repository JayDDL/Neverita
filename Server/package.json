{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm jest --forceExit",
    "start": "node server.js",
    "dev": "dotenvx run -- bun --watch server.ts",
    "build": "tsc",
    "db:reset":"dotenv -e .env -- npx prisma migrate reset",
    "db:migrate": "dotenv -e .env -- npx prisma migrate dev && npx prisma generate",
    "db:seed": "dotenv -e .env -- npx prisma db seed",
    "db": "npm run db:reset && npm run db:migrate && npm run db:seed",
    "db-test:migrate": "dotenv -e .env.test -- npx prisma migrate dev && npx prisma generate",
    "db-test:seed": "dotenv -e .env.test -- npx prisma db seed",
    "db-test": "dotenv -e .env.test -- npx prisma db push --force-reset && npm run db-test:migrate"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "keywords": [],
  "type": "module",
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.6.4",
    "@prisma/client": "5.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/jest": "^29.5.12",
    "bun": "^1.1.21",
    "jest": "^29.7.0",
    "prisma": "^5.17.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  }
}
